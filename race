import turtle as  t
import random as r

screen = t.Screen()

t1 = t.Turtle()
t1.color("red")
t2 = t.Turtle()
t1.color("blue")
t3 = t.Turtle()
t1.color("orange")
t4 = t.Turtle()
t1.color("red")
t5 = t.Turtle()
t1.color("red")

turtles = [t1,t2,t3,t4,t5]
colors = ["red","blue","orange","pink","light green"]

def setup():
    t.penup()
    t.goto(-250, 200)
    t.pendown()
    t.goto(-250, -200)
    
setup()
